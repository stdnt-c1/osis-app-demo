{
  "version": 2,
  "builds": [
    {
      "src": "**/*.html",
      "use": "@vercel/static-build"
    },
    {
      "src": "assets/**/*",
      "use": "@vercel/static-build"
    },
    {
      "src": "manifest.json",
      "use": "@vercel/static-build"
    },
    {
      "src": "service-worker.js",
      "use": "@vercel/static-build"
    }
  ],
  "routes": [
    {
      "src": "/",
      "dest": "/index.html"
    },
    {
      "src": "/(.*)",
      "dest": "/$1"
    }
  ],
  "env": {
    "SUPABASE_URL": "@supabase_url",
    "SUPABASE_ANON_KEY": "@supabase_anon_key",
    "BLOB_READ_WRITE_TOKEN": "@blob_read_write_token"
  }
}
